TipoRet DELETE_ARCHIVO_ARCHIVOS(archivos & as, char * nombreArchivo){
// Elimina un archivo del conjunto de archivos
	char * nombre, * ext;
	nombre = strtok (nombreArchivo,".");
	if (nombre != NULL){
		ext = strtok (NULL, ".");
		if (ext == NULL){// si no tiene extencion, crea una vac√≠a
			ext = new(char[strlen("")]);
			strcpy(ext, "");
		}
	}
	if (PERTENECE_ARCHIVO_ARCHIVOS (as, nombre, extension)){
		as = BORRAR_ARCHIVO_ARCHIVOS(as, nombre, extension);
		printf("\nArchivo eliminado con exito.\n");
		return OK;
	}else{
		printf("\nEl archivo no existe.\n");
		return ERROR;
	}
}

